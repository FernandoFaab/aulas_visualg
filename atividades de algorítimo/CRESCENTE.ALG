Algoritmo "crescente"

Var

   x , y : inteiro

Inicio

   escreval ("Digite dois números: ")
   leia (x)
   leia (Y)
   
   enquanto x <> y faca
   
      se x > y entao
         escreval ("DECRESCENTE!")
      senao
         escreval ("CRESCENTE!")
      fimse
      
      escreval ("Digite outros dois números")
      leia (x)
      leia (Y)
      
   fimenquanto

Fimalgoritmo