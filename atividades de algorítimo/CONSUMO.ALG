Algoritmo "Consumo"

Var

   distancia, consumoMedio, combustivelGasto: real
   

Inicio

    escreva ("Distancia percorrida: ")
    leia (distancia)
    escreva ("combustivel gasto: ")
    leia (combustivelGasto)

    consumoMedio <- distancia / combustivelGasto
    
    escreva ("Consumo medio = ", consumoMedio:8:3)

Fimalgoritmo